import time

def log_time_function(func):
    def wrapper(*args,**kwargs):
        start = time.time()
        result = func(*args,**kwargs)
        end=time.time()
        print(f"{func.__name__} will take {(end-start)} seconds to run")
        
        return result
    return wrapper

@log_time_function
def square_num(numbers) :
    i=1
    while i<10:
        print(f"{i}Ã—{i}=",i*i)
        i+=1
    
        
square_num(1000)
             
